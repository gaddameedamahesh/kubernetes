kops:curl -LO https://github.com/kubernetes/kops/releases/download/$(curl -s https://api.github.com/repos/kubernetes/kops/releases/latest | grep tag_name | cut -d '"' -f 4)/kops-linux-amd64
chmod +x kops-linux-amd64
sudo mv kops-linux-amd64 /usr/local/bin/kops
kops version
kubectl:curl -LO "https://dl.k8s.io/release/$(curl -L -s https://dl.k8s.io/release/stable.txt)/bin/linux/amd64/kubectl"
chmod +x ./kubectl
sudo mv ./kubectl /usr/local/bin/kubectl
kubectl version --client
aws configure
s3 bucket:aws s3api create-bucket \
--bucket your-name-kops-state-store \
--region us-east-1
export KOPS_STATE_STORE=s3://your-name-kops-state-store
ssh-keygen
 kops create cluster --name mahesh.k8s.local --zones us-east-1a --master-size t2.medium --node-size t2.micro
 kops update cluster --name mahesh.k8s.local --yes --admin
